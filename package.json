{
  "name": "parts-viewer",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "description": "Parts Viewer desktop app for managing parts with history, Excel export, and statistics",
  "author": "Closeseyes",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tsc:electron": "tsc -p tsconfig.electron.json",
    "electron": "npm run tsc:electron && node ./node_modules/electron/cli.js dist-electron/main.js",
    "electron-dev": "concurrently \"vite --port 5173\" \"wait-on http://localhost:5173 && npm run tsc:electron && node ./node_modules/electron/cli.js dist-electron/main.js --dev\"",
    "electron-build": "npm run build && npm run tsc:electron && electron-builder"
  },
  "dependencies": {
    "@types/papaparse": "^5.5.2",
    "axios": "^1.13.2",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-is-dev": "^3.0.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "wait-on": "^9.0.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "build": {
    "appId": "com.closeseyes.partsviewer",
    "productName": "Parts Viewer",
    "directories": {
      "output": "out-package"
    },
    "asar": false,
    "files": [
      "dist/**",
      "dist-electron/**",
      "package.json",
      "!dist/win-unpacked/**"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
